{
  "name": "hooked",
  "version": "1.0.0",
  "description": "React Hooks abstraction for few common APIs",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/hooked.js && NODE_ENV=production webpack src/index.js dist/hooked.min.js",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nitishkr88/hooked.git"
  },
  "keywords": [
    "hooked-keywords"
  ],
  "author": "Nitish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nitishkr88/hooked/issues"
  },
  "homepage": "https://github.com/nitishkr88/hooked",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "isparta": "^4.1.1",
    "jest": "^24.9.0",
    "react": "^16.11.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "invariant": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8 < 17"
  }
}
